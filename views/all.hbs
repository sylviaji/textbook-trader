<h1>Textbooks</h1>

<table class="table">
    <thead>
    <tr>
        <th scope="col">Action</th>
        <th scope="col">Title</th>
        <th scope="col">ISBN</th>
        <th scope="col">Price</th>
    </tr>
    </thead>
    <tbody>
    {{#each objs}}
        <tr>
            <td>{{this.action}}</td>
            <td>{{this.title}}</td>
            <td>{{this.isbn_13}}</td>
            <td>{{this.price}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>